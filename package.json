{
    "name": "@ipregistry/client",
    "description": "Official Ipregistry Javascript Library (ES5, ES6+, TypeScript).",
    "version": "4.5.0",
    "type": "module",
    "main": "./dist/index.js",
    "browser": "./dist/browser/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/index.d.ts",
    "homepage": "https://ipregistry.co",
    "license": "Apache-2.0",
    "keywords": [
        "country",
        "currency",
        "es5",
        "esm",
        "ip",
        "ip2location",
        "ipaddress",
        "ipdata",
        "ipinfo",
        "ip intelligence",
        "ipinfodb",
        "ipgeo",
        "ip geolocation",
        "ipregistry",
        "ipthreat",
        "ipv4",
        "ipv6",
        "maxmind",
        "nodejs",
        "threat-data",
        "typescript"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ipregistry/ipregistry-javascript.git"
    },
    "bugs": {
        "url": "https://github.com/ipregistry/ipregistry-javascript/issues"
    },
    "scripts": {
        "build:browser": "rollup -c",
        "build:cjs": "./node_modules/.bin/tsc -p tsconfig.cjs.json",
        "build:mjs": "./node_modules/.bin/tsc -d --outDir dist/esm",
        "build": "npm run lint && npm run format && npm run build:mjs && npm run build:cjs && npm run build:browser",
        "clean": "rm -rf dist/",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "lint": "./node_modules/.bin/eslint src/*",
        "prepack": "npm run clean && npm run build",
        "pretest": "npm run clean && npm run build",
        "test:integration": "node --loader ts-node/esm integration_test/ipregistry.ts",
        "tsdoc": "typedoc --out docs ./src/index.ts"
    },
    "author": "Ipregistry <support@ipregistry.co>",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "chai": "^5.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.2.5",
        "rollup": "^4.13.0",
        "ts-node": "^10.9.2",
        "tsdoc": "^0.0.4",
        "tslib": "^2.6.2",
        "typedoc": "^0.25.12",
        "typescript": "^5.4.2"
    },
    "dependencies": {
        "ky": "^1.2.2",
        "lru-cache": "^10.2.0"
    },
    "files": [
        "dist/**"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "engineStrict": true,
    "private": false
}
