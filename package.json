{
  "name": "@ipregistry/client",
  "description": "Official Ipregistry Javascript Library (ES5, ES6+, TypeScript).",
  "version": "4.5.0",
  "type": "module",
  "main": "./dist/index.js",
  "browser": "./dist/browser/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://ipregistry.co",
  "license": "Apache-2.0",
  "keywords": [
    "country",
    "currency",
    "es5",
    "esm",
    "ip",
    "ip2location",
    "ipaddress",
    "ipdata",
    "ipinfo",
    "ipinfodb",
    "ipgeo",
    "ipregistry",
    "ipthreat",
    "ipv4",
    "ipv6",
    "maxmind",
    "nodejs",
    "threat-data",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ipregistry/ipregistry-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/ipregistry/ipregistry-javascript/issues"
  },
  "scripts": {
    "build:browser": "./node_modules/webpack-cli/bin/cli.js ",
    "build:cjs": "./node_modules/.bin/tsc -p tsconfig.cjs.json",
    "build:mjs": "./node_modules/.bin/tsc -d --outDir dist/esm",
    "build": "npm run lint && npm run build:mjs && npm run build:cjs && npm run build:browser",
    "clean": "rm -rf dist/",
    "lint": "./node_modules/.bin/eslint src/*",
    "prepack": "npm run clean && npm run build",
    "pretest": "npm run clean && npm run build",
    "test:integration": "mocha -r ts-node/register integration_test/**/*.ts",
    "tsdoc": "yarn typedoc --out docs ./src/"
  },
  "author": "Ipregistry <support@ipregistry.co>",
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/lru-cache": "^7.10.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "chai": "^5.1.0",
    "mocha": "^10.3.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ky": "^1.2.2",
    "lru-cache": "^10.2.0"
  },
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "private": false
}
